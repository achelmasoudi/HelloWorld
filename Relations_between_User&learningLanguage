MATCH(u:user),(l:language) WHERE u.learningLanguage="English" AND l.language="English" CREATE     
    (u) - [relation:Öğrenir] -> (l) RETURN u,l;

MATCH(u:user),(l:language) WHERE u.learningLanguage="Arabic" AND l.language="Arabic" CREATE     
    (u) - [relation:Öğrenir] -> (l) RETURN u,l;

