    (u) - [relation:Öğrenir] -> (l) RETURN u,l;

MATCH(u:user),(l:language) WHERE u.learningLanguage="French" AND l.language="French" CREATE     
    (u) - [relation:Öğrenir] -> (l) RETURN u,l;

MATCH(u:user),(l:language) WHERE u.learningLanguage="Spanish" AND l.language="Spanish" CREATE     
    (u) - [relation:Öğrenir] -> (l) RETURN u,l;
